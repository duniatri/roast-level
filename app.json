{
  "expo": {
    "name": "Coffee Roast Analyzer",
    "slug": "coffee-roast-analyzer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "coffeeroast",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coffeeroast.analyzer"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F8F5F0",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.coffeeroast.analyzer"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F8F5F0",
          "dark": {
            "backgroundColor": "#1A1412"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Coffee Roast Analyzer to access your camera to photograph coffee beans."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Coffee Roast Analyzer to access your photos to select coffee bean images."
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}
